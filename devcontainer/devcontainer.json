{
  "image": "npokc-fedora-dev:latest",
  "forwardPorts": [],
  "mounts": [
    {
      "source": "pnpm-store",
      "target": "/home/npokc/.local/share/pnpm/store",
      "type": "volume"
    },
    {
      "source": "shell-history",
      "target": "/home/npokc/.local/share/history/",
      "type": "volume"
    }
  ],
  "workspaceMount": "",
  "runArgs": [
    "--volume=${localWorkspaceFolder}:/workspaces/${localWorkspaceFolderBasename}",
    "--network=host"
  ]
}
